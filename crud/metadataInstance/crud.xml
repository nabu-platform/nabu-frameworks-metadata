    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>id</key>
        <operator>=</operator>
        <or>false</or>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>metadataTypeId</key>
        <operator>=</operator>
        <or>false</or>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>metadataTypeName</key>
        <operator>=</operator>
        <or>false</or>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <foreignFields>
