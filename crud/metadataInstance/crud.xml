    <createBlacklistFields>modified</createBlacklistFields>
    <createBlacklistFields>created</createBlacklistFields>
    <createPermission>metadata.manage</createPermission>
    <createRole>$user</createRole>
    <deletePermission>metadata.manage</deletePermission>
    <deleteRole>$user</deleteRole>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>contentId</key>
        <operator>=</operator>
        <or>false</or>
